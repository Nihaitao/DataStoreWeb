                        <div class="layui-input-block">
                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--64 关联产品-->
                    <div class="layui-form-item" v-if="item.FieldType==64">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <button class="layui-btn layui-btn-primary" @click="ChoiceDepart"><i class="layui-icon"></i>关联产品</button>
                            <button class="layui-btn layui-btn-primary" @click="ProductModel"><i class="layui-icon"></i>{{submitNewForm.Value}}</button>
                            <!--<input @dblclick="ProductModel()" class=" layui-input" v-model="submitNewForm.Value" readonly />-->
                        </div>
                    </div>

                    <!--5 关系等级-->
                    <div class="layui-form-item" v-if="item.FieldType==5">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--6 学历-->
                    <div class="layui-form-item" v-if="item.FieldType==6">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--11 省市区地址-->
                    <div v-if="item.FieldType == 11">
                        <div class="layui-form-item">
                            <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                            <!--省份-->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="provinceSetting" v-bind:tag="'Province' + item.ID" v-bind:value.sync="submitNewForm.NowProvince" initial="off" v-bind:change="provinceChange"></remote-sel>
                            </div>
                            <!-- 市 -->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="citySetting" v-bind:tag="'City' + item.ID" v-bind:value.sync="submitNewForm.NowCity" v-bind:change="cityChange" initial="off"></remote-sel>
                            </div>
                            <!-- 区 -->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="areaSetting" v-bind:tag="'Area' + item.ID" v-bind:value.sync="submitNewForm.NowArea" initial="off"></remote-sel>
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>详细地址</label>
                            <div class="layui-input-block">
                                <input lay-verify="{{item.Required == 1 ? 'required' : ''}}" type="text" autocomplete="off" placeholder="请输入详细地址" class="layui-input" v-model="submitNewForm.NowAddress">
                            </div>
                        </div>
                    </div>

                    <!--33 性别-->
                    <div class="layui-form-item" v-if="item.FieldType==33">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-rad v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="SelectSex" v-bind:value.sync="submitNewForm.Sex"
                                        initial="off"></remote-rad>
                        </div>
                    </div>
                </div>
            </div>

            <div class="layui-form-item layui-hide">
                <button class="layui-btn2" lay-submit @click="submit">提交</button>
            </div>
        </div>
    </div>
</body>
</html>

<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/vue/vue.min.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/layui.js"></script>
<link href="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/css/layui.css" rel="stylesheet" />
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/require_config.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/Controls/Common.js"></script>
<script>
    var dictionaryMapping = new Array();
    dictionaryMapping["OpportunityPhase_ID"] = "OpportunityPhase_ID";
    dictionaryMapping["OpportunitySource_ID"] = "OpportunitySource_ID";
    dictionaryMapping["OpportunityType_ID"] = "OpportunityType_ID";

    require_js_file(["table", "form", "jquery", "tableExt", "laydate", "customUtil", "upload"],
        function (fnr) {

            var laydate = layui.laydate;
            laydate.render({
                elem: "#EffDate"
            });
            var $ = layui.jquery;
            var form = layui.form;
            var upload = layui.upload;
            var ProductID = "";
            function trim(str) {
                return str.replace(/(^\s*)|(\s*$)/g, "");
            }
            function isPhoneNum(str) {
                return /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/.test(str)
            }
            Vue.filter('format',
                function (time, format) {
                    if (time == undefined || time == null || !time) {
                        return '';
                    }
                    var finalTime;
                    if (typeof (time) == 'string') {
                        time = time.replace("T", " ");
                        time = time.replace("Z", " ");
                        var timeIOS = time.replace(/\-/g, "/");
                        finalTime = new Date(timeIOS);
                    } else {
                        finalTime = time;
                    }

                    var t = new Date(finalTime);
                    var tf = function (i) { return (i < 10 ? '0' : '') + i };
                    return format.replace(/yyyy|MM|dd|HH|mm|ss/g,
                        function (a) {
                            switch (a) {
                                case 'yyyy':
                                    return tf(t.getFullYear());
                                    break;
                                case 'MM':
                                    return tf(t.getMonth() + 1);
                                    break;
                                case 'mm':
                                    return tf(t.getMinutes());
                                    break;
                                case 'dd':
                                    return tf(t.getDate());
                                    break;
                                case 'HH':
                                    return tf(t.getHours());
                                    break;
                                case 'ss':
                                    return tf(t.getSeconds());
                                    break;
                            }
                        });
                })

            window.rootApp = new Vue({
                el: "#root",
                data: {
                    productCounts:0,
                    upLoadSrc: '123',
                    isDisplay: false,
                    formData: {
                        common: [],
                        other: [],
                        ProductDate: [],
                    },
                    searchConditon:
                       {
                           ID: '0',
                           Amount: '',
                           Remark: '',
                           SalePrice: '',
                           Name: '',
                           ProductNumber: '',
                           ProductTypeName: '',
                           SaleUnit: '',
                           NormalPrice: ''

                       },
                    SelectSex: SelectSex(),

                    submitNewForm: {
                        Value: "你还未选择产品",
                        ProductDate: []
                    },
                    uploadElements: [],
                    ContractStatusSetting: {
                        search: true,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Description' },
                        remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: 'ContractStatus_ID' } }
                    },
                },

                ready: function () {
                    this.loadCustomForm();

                },
                methods: {
                    OpportunitySetting: function () {
                        return {
                            search: true,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Name' },
                            remote: { url: '/service/crm/Opportunity/GetWholeOpportunityList' }
                        }
                    },
                    ClientSetting: function () {
                        return {
                            search: true,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Name' },
                            remote: { url: '/service/crm/Client/GetAllClient' }
                        }
                    },
                    createSetting: function (fieldName) {
                        return {
                            search: true,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Description' },
                            remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: dictionaryMapping[fieldName] } }
                        }
                    },
                    AccountSetting: function () {
                        return {
                            search: true,
                            selectedIndex: 0,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Name' },
                            remote: { url: '/service/crm/Opportunity/GetMyClientList' }
                        }
                    },
                    getSubmitData: function () {
                        var self = this;
                        var submitData = {};
                        var list = [];
                        $.each(self.formData.common,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });

                        $.each(self.formData.other,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });
                        submitData.ListExtData = list;
                        return submitData;
                    },
                    toggleDisplay: function () {
                        this.isDisplay = !this.isDisplay;
                    },
                    loadCustomForm: function () {
                        var self = this;
                        $.ajax({
                            url: '/service/public/CustomSettings/GetLayoutConfig',
                            type: 'get',
                            data: { BusType: 6, Status: 1 },
                            dataType: 'json',
                            success: function (data) {
                                if (data.SuccessResponse) {
                                    var childArr = [];
                                    var newDatas = data.Data;
                                    $.each(newDatas,
                                        function (key, value) {
                                            $.each(value.Childs,
                                                function (childKey, childValue) {
                                                    if (childValue.FieldType == 61 || childValue.FieldType == 63) {
                                                        self.uploadElements[childValue.FieldName] = childValue;
                                                    }

                                                    if (childValue.FieldType == 107) {
                                                        self.uploadElements[childValue.FieldName] = childValue;
                                                    }
                                                    if (childValue.DataSource != null) {
                                                        childValue.DataSource = eval(childValue.DataSource);
                                                        childArr.push(childValue);
                                                    } else {
                                                        childArr.push(childValue);
                                                    }
                                                });
                                        });
                                    $.each(childArr,
                                        function (key, value) {
                                            if (value.FieldType > 0) {
                                                if (value.IsEnable == 1) {
                                                    value.value = '';
                                                    (value.Common == 1 ? self.formData.common : self.formData.other)
                                                        .push(value);
                                                }
                                            }
                                        });

                                }
                            }
                        });
                    },
                    ChoiceDepart: function () {
                        var self = this;
                        fnr.openDialog({
                            title: "请选择产品",
                            content: ["Add.html?Value=" + 1, "yes"],
                            area: ['1400px', (parent.document.documentElement || parent.document.body).clientHeight - 140 + "px"],
                            events: {
                                new: function (data) {
                                    var cuot = "";
                                    var Name = "";
                                    var List = [];
                                    $.each(data.ContractProductDate, function (k, v) {
                                        ProductID += v.ID + ',';
                                        Name += v.Name + ',';
                                        cuot++;
                                        List.push({ ID: v.ID, Amount: v.Amount, Remark: v.Remark, SalePrice: v.SalePrice, Name: v.Name, ProductNumber: v.ProductNumber, ProductTypeName: v.ProductTypeName, SaleUnit: v.SaleUnit, NormalPrice: v.NormalPrice })
                                    })
                                    if (cuot == "") {
                                        cuot = 0
                                    }
                                    self.submitNewForm.Value = "你选择了" + data.ContractProductDate.length + "种产品";
                                    self.submitNewForm.ProductDate = List;
                                }

                            }

                        });
                    },
                    ProductModel: function () {
                        var self = this;
                        var Other = ProductID.substring(0, ProductID.length - 1);
                        window['searchConditon'] = self.formData.OppListDate;
                        fnr.openDialog({
                            title: "查看产品",
                            content: ["Update.html?Other=" + Other + "&ID=" + fnr.getQueryString('ID') + "&Value2=" + 1, "yes"],
                            area: ['1050px', (parent.document.documentElement || parent.document.body).clientHeight - 650 + "px"],
                            btn: ['确认'],
                            events: {
                                refresh: function (data) {
                                    var List = [];
                                    $.each(data.ContractProductDate, function (k, v) {
                                        ProductID += v.ID + ',';
                                        List.push({ ID: v.ID, Amount: v.Amount, Remark: v.Remark, SalePrice: v.SalePrice, Name: v.Name, ProductNumber: v.ProductNumber, ProductTypeName: v.ProductTypeName, SaleUnit: v.SaleUnit, NormalPrice: v.NormalPrice, Client_ID: v.Client_ID, CorrelationID: v.CorrelationID })
                                    })
                                    self.productCounts = data.ContractProductDate.length;
                                    self.formData.ProductDate = List;
                                }
                            }
                        });
                    },
                    submit: function (e) {
                        var self = this;
                        if (!layui.form.checkInput(e)) {
                            return false;
                        }

                        var AddDatas = $.extend(self.getSubmitData(), self.submitNewForm);
                        console.log(AddDatas);
                        $.ajax({
                            url: '/service/crm/Contract/AddContractData',
                            type: 'post',
                            data: AddDatas,
                            dataType: 'json',
                            success: function (data) {
                                if (data.SuccessResponse) {
                                    fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                    setTimeout(function () { fnr.closeDialog(); }, 500);
                                    //fnr.callDialog('refresh',data);
                                    //fnr.closeDialog();
                                } else {
                                    layer.msg("添加失败：" + data.Message);
                                }
                            }
                        });
                    }
                }
            });
        });
</script>