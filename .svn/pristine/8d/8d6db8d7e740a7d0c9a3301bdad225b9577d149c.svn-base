/*弹出层*/
.tanchuBigBox{
	position: fixed;
	top: 0;
	left: 0;width: 100%; height: 100%; background: rgba(0,0,0,.4);
	display: none;
}

.tanchuBox{
	position: fixed;
	top:50%;
	left:50%;
	display: block;
	width: 1000px; 
	height: 700px;
	margin-top: -350px;
	transform: translateX(-50%);
	background: white;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
    overflow-y:auto;
}
.tanchuBox_tit{
	font-size: 16px;
	text-indent: 30px;
	padding: 15px 20px 15px 0px;
}
.tanchuBox ul li{
	float: left;
	width: calc(100% / 4);
	text-align: center;
}
.bottomLine_tchu{
	line-height: 50px;
	border-bottom: 1px dashed #F1F1F1;
}
.Close{
	position: relative;
	display: inline-block;
	width:25px;
	height: 25px;
	background: #9a9a9a;
	border-radius: 50%;
	float: right;
	cursor: pointer;
}
.Close:hover{
	background: #ED4242;
}
.Close:before {
    position: absolute;
    left: 11px;
    top: 4px;
    content: "";
    width: 3px;
    height: 17px;
    background: white;
    transform: rotate(-45deg);
}
.Close:after {
    position: absolute;
    left: 11px;
    top: 4px;
    content: "";
    width: 3px;
    height: 17px;
    background: white;
    transform: rotate(45deg);
 }




