	line-height: 50px;
}

.timeline {
	position: absolute;
	top: 21px;
	left: 7px;
	width: 9px;
	height: 9px;
	margin-bottom: 34px;
	background: #dddddd;
	border-radius: 50%;
}

.datalist .time {
	position: absolute;
	top: 18px;
	left: -13px;
	width: 8px;
	height: 8px;
	background: #dddddd;
	border: 1px solid #F1F1F1;
	border-radius: 50%;
}

.datalist .time:after {
	position: absolute;
	top: -39px;
	left: 3px;
	content: "";
	display: block;
	width: 1px;
	height: 39px;
	border-left: 1px solid #dddddd;
}

.Paylist div.datalist {
	position: relative;
	padding: 12px 0px 10px 15px;
	font-size: 14px;
	color: #999999;
}

.Paylist div.datalist time,
.Paylist div.datalist p {
	display: inline-block;
	padding-right: 12px;
}

