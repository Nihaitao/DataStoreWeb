                                                            <a v-for="(zindex,zitem) in cur.data" href="#{{zitem.ID}}" class="e_answerCard page-card-item" v-bind:class="[zitem.MyAnswer.length>0?(zitem.Judge==1?'right':'error'):'normal']">{{idx+1}}-{{zindex+1}}</a>
                                                        </template>
                                                        <a v-if="item.QuestionType_ID!=7" v-for="(idx,cur) in item.QuestionList" href="#{{cur.ID}}" class="e_answerCard page-card-item" v-bind:class="[cur.MyAnswer.length>0?(cur.Judge==1?'right':'error'):'normal']">{{idx+1}}</a>
