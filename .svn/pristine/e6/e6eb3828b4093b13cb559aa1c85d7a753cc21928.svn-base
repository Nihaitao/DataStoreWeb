<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <link href="~/Resources/css/layui.css" rel="stylesheet" />
    <script src="~/Resources/js/jquery.js"></script>
    <script src="~/Resources/js/layer.js"></script>

    <script>
        $(function () {
            layui.use(['tree', 'laydate', 'layer', 'form', 'laypage', 'layedit', 'element'], function () {
                var $ = layui.jquery;
                var element = layui.element();
                var laydate = layui.laydate;
                var layer = layui.layer;
                var form = layui.form();
                var layedit = layui.layedit;


                //laypage
                var laypage = layui.laypage({
                    cont: $('#demoPage'),
                    pages: 100,
                    skip: true
                });

                //layedit
                layedit.build('demo', {
                    tool: [
                      'strong' //加粗
                      , 'italic' //斜体
                      , 'underline' //下划线
                      , 'del' //删除线

                      , '|' //分割线

                      , 'left' //左对齐
                      , 'center' //居中对齐
                      , 'right' //右对齐

                      , '|' //分割线

                      , 'link' //超链接
                      , 'unlink' //清除链接
                      , 'image' //插入图片
                    ]
                });


                layedit.build('ejectEdit', {
                    tool: [
                      'link' //超链接
                      , 'image' //插入图片
                    ],
                    height: 200
                })

                //trees
                var options = {
                    elem: '#trees',
                    nodes: [
                        {
                            name: '自学考试',
                            children: [
                                {
                                    name: '湖南省',
                                    children: [
                                        {
                                            name: '湖南大学',
                                            children: [
                                                {
                                                    name: '本科',
                                                    children: [
                                                        {
                                                            name: '经济法学'
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            name: '湘潭大学',
                                            children: [
                                                {
                                                    name: '本科'
                                                },
                                                {
                                                    name: '专科'
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    name: '贵州省'
                                }
                            ]
                        },
                        {
                            name: '成人高考',
                            children: [
                                {
                                    name: '湖南省',
                                    children: [
                                        {
                                            name: '湖南大学'
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            name: '远程教育',
                            children: [
                                {
                                    name: '郑州大学'
                                }
                            ]
                        },
                        {
                            name: '电大',
                            children: [
                                {
                                    name: '中央广播电视大学'
                                }
                            ]
                        },
                        {
                            name: '职业资格',
                            children: [
                                {
                                    name: '导游证'
                                }
                            ]
                        }
                    ]
                };
                layui.tree(options);

                //form
                form.on('submit(formDemo)', function (data) {
                    layer.msg(JSON.stringify(data.field));
                    return false;
                });

                //全选
                form.on('checkbox(allChoose)', function (data) {
                    var child = $(data.elem).parents('table').find('tbody input[type="checkbox"]');
                    child.each(function (index, item) {
                        item.checked = data.elem.checked;
                    });
                    form.render('checkbox');
                });

                //layer
                layer.msg('<h4>内容1</h4>');
                $('.btn1').on('click', function () {
                    layer.alert('<h4>内容1</h4>');
                });

                //jquery
                $('.mainForm .btnDate').on('click', function () {
                    $(this).removeClass('btns-default')
                        .addClass('btns-primary');
                    $(this).siblings('.btnDate').removeClass('btns-primary').addClass('btns-default')
                });
                $('.tab-menu ul li').on('click', function () {
                    if (!$(this).hasClass('this')) {
                        $(this).addClass('this');
                        $(this).siblings('li').removeClass('this')
                    }
                });
                $('.layui-colla-title').on('click', function () {
                    if (!$(this).next().hasClass('layui-show')) {
                        $(this).next().addClass('layui-show');
                        $(this).parent().siblings('.layui-colla-item').children('.layui-colla-content').removeClass('layui-show');
                    }
                });

                //laydate
                var start = {
                    min: laydate.now()
                    , max: '2099-06-16 23:59:59'
                    , istoday: false
                    , choose: function (datas) {
                        end.min = datas;
                        end.start = datas
                    }
                };
                var end = {
                    min: laydate.now()
                    , max: '2099-06-16 23:59:59'
                    , istoday: false
                    , choose: function (datas) {
                        start.max = datas;
                    }
                };
                document.getElementById('LAY_demorange_s').onclick = function () {
                    start.elem = this;
                    laydate(start);
                };
                document.getElementById('LAY_demorange_e').onclick = function () {
                    end.elem = this;
                    laydate(end);
                }

                $('.hoverBtn').on('click', function () {
                    $('.dropList').slideToggle();
                });
                $('.dropList a').on('click', function () {
                    if (!$(this).parent().hasClass('hoverColor')) {
                        $(this).parent().addClass('hoverColor')
                            .siblings().removeClass('hoverColor');
                    }
                });

            });
        });
    </script>
</head>
