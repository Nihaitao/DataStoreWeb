.Pro-switch{
    display: none;
}
.Pro-switch + label{
    display: inline-block;
    width: 38px;
    background-color: #fff;
}
.Pro-switch + label:after{
    content: '';
    width: 100%;
    height: 5px;
    position: absolute;
    top:50%;
    border-radius: 3px;
    background-color: #ddd;
    left: 0;
    transform: translate(0,-50%);
}
.Pro-switch + label:before{
    content: '';
    display: block;
    position: absolute;
    width: 16px;
    height: 16px;
    background-color: #fff;
    border: 2px solid #ddd;
    border-radius: 50%;
    top:50%;left: 0;
    z-index: 99;
    transition: all .3s ;
    transform: translate(0,-50%);
}
.Pro-switch:checked + label:before{
    left: 30px;
    border-color: #FD4C6A;
}
.Pro-switch:checked + label:after{
   background-color: #F5CDD6;
}


