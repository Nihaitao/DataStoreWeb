function CheckData(rsp) {
    try {
        if (typeof (rsp) == 'object' && rsp.code && rsp.code == -1) {
            if (top !== self)
                top.location.href = rsp.loginUrl;
            else
                parent.location.href = rsp.loginUrl;
            return false;
        } else {
            if (rsp.error != undefined) {
                if (typeof (rsp) == 'object' && rsp.error) {
                    return true;
                } else {
                    layer.msg(rsp.message);
                    return false;
                }
            } else
                return true;
        }
    } catch (e) { }
}


