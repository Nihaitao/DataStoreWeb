iframe{
    width: 100%;
    height: 100%;
    border: none;
    min-width: 320px;
    position: absolute;
}
body{
    overflow: hidden;
}
.mainTab .layui-tab-title li:first-child i{
    display: none;
}
.mainTab .layui-tab {
    display: inline-block;
    width: 100%;
}
.closeBox {
    display: inline-block;
    width: 120px;
    padding-right:20px;
    position: absolute;
        top:0px;
    right: 0;
    background-color: #fff !important;
    color: #000;
    border-left: 1px solid #e2e2e2;
    border-bottom: 1px solid #e2e2e2;
}
.closeBox .layui-nav-item{
    line-height: 40px;
    width: 100%;
}
.closeBox .layui-nav-item a{
    text-align: center;
    height: 40px;
}
.closeBox .layui-nav-item dd a{
    padding: 0 12px;
    height: 37px;
}
.closeBox .layui-nav-item a,
.closeBox .layui-nav-item a:hover{
    color: #000;
}
.closeBox .layui-nav-child {
    top: 43px;
    left: 5px;
    width:115px;
}
.closeBox.layui-nav .layui-this{
    background-color: transparent;
}
.closeBox.layui-nav dd.layui-this a{
    color: #333;
}
.closeBox a .layui-nav-more{
    top: 17px;
    right: 5px;
    border-color: #000 transparent transparent;
}
.closeBox a .layui-nav-mored{
    top: 13px;
    border-style: dashed dashed solid;
    border-color: transparent transparent #000;
}
.closeBox .layui-nav-bar,
.closeBox .layui-nav-bar:hover {
    opacity: 0;
}
.closeBox .layui-nav{
    background-color: #fff;
}
.closeBox .layui-nav-item dl.layui-nav-child{
    min-width: 0;
    padding: 0;
}