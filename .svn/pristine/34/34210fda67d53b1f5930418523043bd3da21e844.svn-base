                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--64 关联产品-->
                    <div class="layui-form-item" v-if="item.FieldType==64">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <button class="layui-btn layui-btn-primary" @click="ChoiceDepart"><i class="layui-icon"></i>关联产品</button>
                            <button class="layui-btn layui-btn-primary" @click="ProductModel"><i class="layui-icon"></i>你已选择了{{productCounts}}种产品</button>
                        </div>
                    </div>

                    <!--5 关系等级-->
                    <div class="layui-form-item" v-if="item.FieldType==5">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--6 学历-->
                    <div class="layui-form-item" v-if="item.FieldType==6">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="createSetting(item.FieldName)" v-bind:value.sync="item.value"
                                        initial="off"></remote-sel>
                        </div>
                    </div>

                    <!--11 省市区地址-->
                    <div v-if="item.FieldType == 11">
                        <div class="layui-form-item">
                            <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                            <!--省份-->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="provinceSetting" v-bind:tag="'Province' + item.ID" v-bind:value.sync="submitNewForm.NowProvince" initial="off" v-bind:change="provinceChange"></remote-sel>
                            </div>
                            <!-- 市 -->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="citySetting" v-bind:tag="'City' + item.ID" v-bind:value.sync="submitNewForm.NowCity" v-bind:change="cityChange" initial="off"></remote-sel>
                            </div>
                            <!-- 区 -->
                            <div class="layui-input-inline">
                                <remote-sel v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="areaSetting" v-bind:tag="'Area' + item.ID" v-bind:value.sync="submitNewForm.NowArea" initial="off"></remote-sel>
                            </div>
                        </div>
                        <div class="layui-form-item">
                            <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>详细地址</label>
                            <div class="layui-input-block">
                                <input lay-verify="{{item.Required == 1 ? 'required' : ''}}" type="text" autocomplete="off" placeholder="请输入详细地址" class="layui-input" v-model="submitNewForm.NowAddress">
                            </div>
                        </div>
                    </div>

                    <!--33 性别-->
                    <div class="layui-form-item" v-if="item.FieldType==33">
                        <label class="layui-form-label"><span v-if="item.Required == 1" style="color: red;margin-right: 5px;">*</span>{{item.ShowName}}</label>
                        <div class="layui-input-block">
                            <remote-rad v-bind:layverify="item.Required == 1 ? 'required' : ''" v-bind:setting="SelectSex" v-bind:value.sync="submitNewForm.Sex"
                                        initial="off"></remote-rad>
                        </div>
                    </div>
                </div>
            </div>

            <div class="layui-form-item layui-hide">
                <button class="layui-btn" lay-submit @click="submit">提交</button>
            </div>
        </div>
    </div>
</body>
</html>

<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/vue/vue.min.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/layui.js"></script>
<link href="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/css/layui.css" rel="stylesheet" />
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/require_config.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/Controls/Common.js"></script>
<script>
    var dictionaryMapping = new Array();
    dictionaryMapping["Source_ID"] = "Source_ID";
    dictionaryMapping["PhaseType_ID"] = "PhaseType_ID";
    dictionaryMapping["Importance_ID"] = "Importance_ID";
    dictionaryMapping["ConnectionLevel_ID"] = "ConnectionLevel_ID";
    dictionaryMapping["Education_ID"] = "Education_ID";

    require_js_file(["table", "form", "jquery", "tableExt", "laydate", "customUtil", "upload"],
    function (fnr) {

        var laydate = layui.laydate;
        laydate.render({
            //elem: "#text3",
            type: 'data',
            format: 'yyyy-MM-dd' //可任意组合
        });

        var ProductID = "";
        var $ = layui.jquery;
        var upload = layui.upload;
        function trim(str) {
            return str.replace(/(^\s*)|(\s*$)/g, "");
        }
        function isPhoneNum(str) {
            return /^1[3|4|5|7|8][0-9]{9}$/.test(str)
        }
        window.rootApp = new Vue({
            el: "#root",
            data: {
                isDisplay: false,
                productCounts:0,
                submitNewForm: {
                    Sex: '1', 
                    Value: '',
                },
                submitForm: {
                    ID: ''
                },
                submitModify: {
                    ID: GetQueryString('ID'),
                    SystemGroup_ID: GetQueryString('SystemGroup_ID') ? GetQueryString('SystemGroup_ID') : 0
                },
                formData: {
                    common: [],
                    other: [],
                    OppListDate: []
                },
                SelectSex: SelectSex(),
                provinceSetting: SelectProvinceBind(),//省份
                citySetting: SelectCityBind(),//城市
                areaSetting: SelectDistrictBind(),//区域
                NewInfoData: [],
                modifyArr: [],
                uploadElements: [],
                ContractStatusSetting: {
                    search: true,
                    options: [],
                    fields: { root: 'Data', key: 'ID', value: 'Description' },
                    remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: 'ContractStatus_ID' } }
                },
            },
            ready: function () {
                this.submitForm.ID = GetQueryString('ID');
                this.modifyClients();
                this.GetListCuont();
            },
            methods: {
                OpportunitySetting: function () {
                    return {
                        search: true,
                        selectedIndex: 0,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Name' },
                        remote: { url: '/service/crm/Opportunity/GetWholeOpportunityList' }
                    }
                },
                ClientSetting: function () {
                    return {
                        search: true,
                        selectedIndex: 0,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Name' },
                        remote: { url: '/service/crm/Client/GetAllClient' }
                    }
                },
                cityChange: function (o) {
                    var index = o.tag.replace('City', '');
                    var areaTag = 'Area' + index;
                    var self = this;
                    if (self.submitNewForm.NowCity == "") {
                        self.areaSetting.remote.url = "";
                        self.areaSetting.remote.data = {};
                    } else {
                        fnr.mergeJson(SelectDistrictBind(self.submitNewForm.NowCity), self.areaSetting);//合并ajax值
                    }
                    self.submitNewForm.NowArea = "";
                    fnr.findComponent(self, areaTag).reset();
                },

                createSetting: function (fieldName) {
                    return {
                        search: true,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Description' },
                        remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: dictionaryMapping[fieldName] } }
                    }
                },

                toggleDisplay: function () {
                    this.isDisplay = this.isDisplay ? false : true
                },
                //修改商机
                submit: function (e) {
                    var self = this;
                    if (!layui.form.checkInput(e)) {
                        return false;
                    }
                    var list = [];
                    $.each(self.formData.common, function (k, v) {
                        if (v.FieldName.toString().indexOf("customField_") == 0) {
                            list.push({ ItemName: v.FieldName, ItemValue: v.value });
                        }
                        self.submitModify[v.FieldName] = trim(v.value + '');
                    });

                    $.each(self.formData.other, function (k, v) {
                        if (v.FieldName.toString().indexOf("customField_") == 0) {
                            list.push({ ItemName: v.FieldName, ItemValue: v.value });
                        }
                        self.submitModify[v.FieldName] = trim(v.value + '');
                    });

                    self.submitModify.ListExtData = list;
                    self.submitModify.ProductDate = self.formData.OppListDate

                    $.ajax({
                        url: '/service/crm/Contract/UpdeteContract',
                        type: 'post',
                        data: self.submitModify,
                        dataType: 'json',
                        success: function (data) {
                            if (data.SuccessResponse) {
                                fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                setTimeout(function () { fnr.closeDialog(); }, 500);
                            }
                        }
                    });
                },
                uploadRender: function () {
                    var self = this;
                    layui.$(".uploadBtn").each(function (k, v) {
                        var fieldName = $(v).data("fieldname");
                        var UploadInst = upload.render({
                            elem: v
                        , url: '/service/public/Upload/File/2'
                        , before: function (obj) {
                            //预读本地文件示例，不支持ie8
                            obj.preview(function (index, file, result) {
                                $(v).prev().attr('src', result); //图片链接（base64
                            });
                        }
                        , done: function (res) {
                            //如果上传失败
                            if (res.code > 0) {
                                return layer.msg('上传失败');
                            }
                            self.uploadElements[fieldName].value = res.Message;
                            //上传成功
                            $(v).prev().prev().val(res.Message);
                            //rootApp.HeaderSubmitForm.Favicon = res.Message
                            layer.msg('上传成功')
                        }
                        , error: function () {
                            //演示失败状态，并实现重传
                            var demoText = $('#upLoadText' + id);
                            $(v).next().html('<span style="color: #FF5722;">上传失败</span> <a class=" favicon-reload" style="cursor:pointer;">重试</a>');
                            $(v).next().find('.favicon-reload').on('click', function () {
                                UploadInst.upload();
                            });
                        }
                        });
                    });
                },
                //获取合同详细信息
                modifyClients: function () {
                    var self = this;
                    $.ajax({
                        url: '/service/crm/Contract/GetContractInfo',
                        type: 'GET',
                        dataType: 'json',
                        data: { ID: GetQueryString('ID') },
                        success: function (data) {
                            if (data.SuccessResponse) {
                                var Datas = data.Data;
                                var childArr = [];
                                $.each(Datas, function (k, v) {
                                    if (v.Childs.length > 0) {
                                        $.each(v.Childs, function (_k, _v) {
                                            if (_v.FieldType == 61 || _v.FieldType == 63) {
                                                self.uploadElements[_v.FieldName] = _v;
                                            }
                                            if (_v.FieldType == 108) {
                                                if (_v.Value != '0001/1/1 0:00:00') {
                                                    _v.Value = fnr.format(_v.Value, 'yyyy-MM-dd');
                                                }
                                                else {
                                                    _v.Value = "";
                                                }
                                            }
                                            if (_v.DataSource != null) {
                                                
                                                _v.DataSource = eval(_v.DataSource);
                                            }
                                            childArr.push(_v);
                                            if (_v.FieldName !== null) {
                                                if (_v.FieldName.split('_').length > 1 && _v.FieldName.toString().indexOf('customField_') != 0) {
                                                    if (_v.Value != null) {
                                                        self.submitNewForm[_v.FieldName] = parseFloat(_v.Value.split(',')[1]);
                                                    }
                                                } else if (_v.FieldName !== 'Sex') {
                                                    self.submitNewForm[_v.FieldName] = _v.Value == 0 ? '' : _v.Value;
                                                } else {
                                                    self.submitNewForm[_v.FieldName] = _v.Value;
                                                }
                                            }

                                        })
                                    }
                                })
                                $.each(childArr, function (k, v) {
                                    if (v.FieldType > 0) {

                                        v["value"] = self.submitNewForm[v.FieldName];
                                        if (isNaN(v["value"])) {

                                            v["Value"] = '';
                                        }
                                        if (v.IsEnable == 1) {
                                            (v.Common == 1 ? self.formData.common : self.formData.other)
                                                .push(v);
                                        }
                                    }

                                });
                                self.$nextTick(function () {
                                    self.uploadRender();
                                });
                            }
                        },
                        error: function () {
                            alert("error");
                        }
                    });
                },
                //获取产品数量
                GetListCuont: function () {
                    var self = this;
                    $.get("/service/crm/Contract/GetProductList", { ID: fnr.getQueryString('ID') }, function (resp) {
                        if (resp.SuccessResponse) {
                            self.formData.OppListDate = resp.Data;
                            $.each(resp.Data, function (k, v) {
                                ProductID += v.CorrelationID + ',';
                            })
                            self.productCounts = resp.Data.Length;
                        }
                    },
                       "json");
                },
                //择产品
                ChoiceDepart: function () {
                    var self = this;
                    fnr.openDialog({
                        title: "请选择产品",
                        content: ["Add.html?ID="+ fnr.getQueryString('ID')+"&Value1=" + 1 , "yes"],
                        area: ['1400px', (parent.document.documentElement || parent.document.body).clientHeight - 250 + "px"],
                        events: {
                            new: function (data) {
                                var List = [];
                                $.each(data.ContractProductDate, function (k, v) {
                                    ProductID += v.ID + ',';
                                    List.push({ ID: v.ID, Amount: v.Amount, Remark: v.Remark, SalePrice: v.SalePrice, Name: v.Name, ProductNumber: v.ProductNumber, ProductTypeName: v.ProductTypeName, SaleUnit: v.SaleUnit, NormalPrice: v.NormalPrice, Client_ID: v.Client_ID, CorrelationID: v.CorrelationID })
                                })
                                self.productCounts = data.ContractProductDate.length;
                                self.formData.OppListDate = List;
                            }
                        }
                    });
                },
                ProductModel: function () {
                    var self = this;
                    var Other = ProductID.substring(0, ProductID.length - 1);
                    window['searchConditon'] = self.formData.OppListDate;
                    fnr.openDialog({
                        title: "查看产品",
                        content: ["Update.html?Other=" + Other + "&ID=" + fnr.getQueryString('ID') + "&Value2=" + 1, "yes"],
                        area: ['1400px', (parent.document.documentElement || parent.document.body).clientHeight - 250 + "px"],
                        btn: ['确认'],
                        events: {
                            refresh: function (data) {
                                var List = [];
                                $.each(data.ContractProductDate, function (k, v) {
                                    ProductID += v.ID + ',';
                                    List.push({ ID: v.ID, Amount: v.Amount, Remark: v.Remark, SalePrice: v.SalePrice, Name: v.Name, ProductNumber: v.ProductNumber, ProductTypeName: v.ProductTypeName, SaleUnit: v.SaleUnit, NormalPrice: v.NormalPrice, Client_ID: v.Client_ID, CorrelationID: v.CorrelationID })
                                })
                                self.productCounts = data.ContractProductDate.length;
                                self.formData.OppListDate = List;
                            }
                        }
                    });
                },


            }
        });
    });
</script>