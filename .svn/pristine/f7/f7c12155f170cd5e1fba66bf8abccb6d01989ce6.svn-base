*{
    margin: 0;
    padding: 0;
}

@font-face {
    font-family: PingFang-SC-Semibold;
    src: url("fonts/PingFang-SC-Semibold.ttf");
}

@font-face {
    font-family: PingFangSC-Regular ;
    src: url("fonts/PingFang_Regular.otf");
}
body{font-family: PingFangSC-Regular,sans-serif;}
.resetIcon{
    display: inline-block;
    font-size: 0.28rem;
}
.reIcon{
    display: inline-block;
    font-size: 0.56rem;
    color: #FFBE00;
}
a:hover{
    text-decoration: none;
}
.heightMax{
    height: 100%;
}
.lines{
    box-sizing: border-box;
    height: 0.2rem;
    border-top: 0.01rem solid #f2f2f2;
    background-color: #f5f5f9;
}
.bottomBox{
    width: 100%;
    height: 0.98rem;
    background-color: #f7f6fb;
    position: fixed;
    bottom: 0;
}
.reportText{
    font-size: 0.22rem;
    /*color: #f70968;*/
    color: #999;
}
.reportIcon,
.fnIcon,
.needToIcon,
.helpIcon{
    font-size: 0.54rem;
    color: #999;
}
.iconBox.active .reportText{
    color: #f70968;
}
.iconBox.active .reportIcon{
    color: #f70968;
}
.middleIcon{
    font-size: 0.86rem;
    color: #CECED6;
    display: inline-block;
    height: 100%;
}
.iconBox{
    text-align: center;
    margin-top: 0.08rem;
}
.leftDiv{
    border-right: 0.01rem solid #e5e5e5;
    box-sizing: border-box;
}
.leftDiv,
.rightDiv{
    height: 100%;
    line-height: 0.96rem;
    text-align: center;
}
.btnText{
    display: inline-block;
    font-size: 0.28rem;
    margin-right: 0.03rem;
    color: #333;
}
.selectBtnBox{
    height: 0.96rem;
    border-bottom: 0.01rem solid #e5e5e5;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background-color: #fff;
}