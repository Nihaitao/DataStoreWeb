    <script>
        jQuery.fn.slideLeftHide = function (speed, callback) {
            this.animate({
                width: "hide",
                paddingLeft: "hide",
                paddingRight: "hide",
                marginLeft: "hide",
                marginRight: "hide"
            }, speed, callback);
        };
        jQuery.fn.slideLeftShow = function (speed, callback) {
            this.animate({
                width: "show",
                paddingLeft: "show",
                paddingRight: "show",
                marginLeft: "show",
                marginRight: "show"
            }, speed, callback);
        };
        
    </script>
    <script>
        var player;
        //截取URL地址栏参数
        function GetQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }
        function IsPC() {
            var userAgentInfo = navigator.userAgent;
            var Agents = ["Android", "iPhone",
                        "SymbianOS", "Windows Phone",
                        "iPad", "iPod"];
            var flag = true;
            for (var v = 0; v < Agents.length; v++) {
                if (userAgentInfo.indexOf(Agents[v]) > 0) {
                    flag = false;
                    break;
                }
            }
            return flag;
        }
        $(function () {
            var C_ID;
            var P_ID;

            function cName() {
                $.get('/service/NoAuthority/GetCourseByID', { ID: GetQueryString('Course_ID') }, function (data) {
                    if (data.SuccessResponse) {
                        $('.coursePrice').html(data.ReturnData.Data.Price.toFixed(2));
                        $('.buyCourseBtn').data('id', 1 + '');
                        $('.buyPeNum').html(data.ReturnData.Data.BuyBase);
                        var isPay = data.ReturnData.Data.IsPay;
                        var data = {
                            list: data.ReturnData.Data
                        }
                        var html = template('cName-text', data);
                        var html1 = template('title-text', data);
                        var html2 = template('buyCourse', data);
                        document.getElementById('cName-chapter').innerHTML = html;
                        $('title').html(html1);
                        $('.footerBox').html(html2);
                        catalog(isPay);
                    }
                })
            }

            cName()
            //左侧菜单
            function catalog(txt) {
                var Course_ID = GetQueryString('Course_ID');
                $.get('/service/NoAuthority/GetCourse_ChaptersNodeList', { Course_ID: Course_ID }, function (data) {
                    if (data.SuccessResponse) {
                        var Datas = data.ReturnData.Data;
                        var data = {
                            dataTwo: {
                                IsPay: txt,
                                list: Datas
                            }
                        }
                        var html = template('Video-text', data);
                        document.getElementById('Video-chapter').innerHTML = html;
                        setTimeout(function () {
                            $(".Video-Catalog").mCustomScrollbar({
                                mouseWheel: true,
                            });
                            $(".tab-content .active .Video-sro").mCustomScrollbar({
                                mouseWheel: true,
                            });
                        },0);
                        var liArr = $('.Video-Section li.elseFree');
                        var ID = GetQueryString('ID');
                        for (var s = 0; s < liArr.length; s++) {
                            if ($(liArr[s]).data('id') == ID) {
                                $(liArr[s]).addClass('learning');
                                $(liArr[s]).find('a span').css('display', 'none');
                                $(liArr[s]).find('i.iIcon').removeClass('fa-video-camera').addClass('fa-youtube-play');
                                courseWare($(liArr[s]).data('id'));
                            }
                        }
                        //点击视频切换
                        $('.Video-Section').on('click', 'li.elseFree', function () {
                            var _thisId = $(this).data('id');
                            location.href = 'Video.shtml?ID=' + _thisId + '&Course_ID=' + GetQueryString('Course_ID');
                            var spanTxt = $(this).data('isopen');
                            if ($(this).find('i.iIcon').hasClass('fa-video-camera')) {
                                $(this).find('i.iIcon').removeClass('fa-video-camera').addClass('fa-youtube-play');
                                $(this).siblings().find('i.iIcon').addClass('fa-video-camera').removeClass('fa-youtube-play');
                                $(this).parents('li').siblings().find('i.iIcon').removeClass('fa-youtube-play').addClass('fa-video-camera');
                            }
                            if (!$(this).hasClass('learning')) {
                                $(this).addClass('learning').siblings('li').removeClass('learning');
                                $(this).parents('li').siblings().find('ul li').removeClass('learning');
                            }
                            C_ID = $(this).data('id');
                            P_ID = $(this).data('pid');
                            courseWare(C_ID);
                        });
                        $('.Video-Section').on('click', 'li.notFree', function () {
                            layer.open({
                                content: '视频未购买，不能观看'
                            })
                        });
                    } else {
                        layer.msg(data.Message);
                    }
                })
            }
            function toggleCatalog() {
                $('.Video-listButton').on('click', function () {
                    var _this = $('.Video-Catalog');
                    _this.slideToggle();
                });
            }

            function courseWare(cId) {
                var OverFlowAuth = '';  //试听视频权限 0所有用户 1注册用户
                var UserSeeAuth = '';  //免费视频/资源观看权限 0所有用户 1注册用户
                $.get('/service/WConfiguration/GetWConfiguration', function (res) {
                    console.log(res);
                    if(res.SuccessResponse){
                        OverFlowAuth = res.ReturnData.Data.OverFlowAuth;
                        UserSeeAuth = res.ReturnData.Data.UserSeeAuth;
                    }
                });
                if (cId) {
                    $.get('/service/Resource/GetCourseWByID', { id: cId, Course_ID: GetQueryString('Course_ID') }, function (data) {
                        var Datas = data.ReturnData.Data;
                        $('.previewTime').html(Datas.OverFlowTime);
                        if (data.SuccessResponse) {
                            if(Datas.IsOpen === 3){  //收费
                                if(Datas.IsPay >= 1){
                                    var pId = Datas.PolyvVID;
                                    var sId = Datas.StuId;
                                    if (IsPC() == true) {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'session_id': sId
                                        });
                                    } else {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'df': 1,
                                            'flashvars': { 'autoplay': 'true', 'param2': sId },
                                            'params': { 'param2': sId },
                                            'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                        });
                                    }
                                    //页面加载时调用
                                    LookCourse(1);
                                    //看视频的时候 用到
                                    setInterval(LookCourse, 60000);//60秒执行一次
                                }else{
                                    layer.msg('视频未购买，不允许观看')
                                }
                            }else if(Datas.IsOpen === 2){   //试听
                                if(Datas.IsPay >= 1){
                                    //试听已购买
                                    var pId = Datas.PolyvVID;
                                    var sId = Datas.StuId;
                                    if (IsPC() == true) {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'session_id': sId
                                        });
                                    } else {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'df': 1,
                                            'flashvars': { 'autoplay': 'true', 'param2': sId },
                                            'params': { 'param2': sId },
                                            'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                        });
                                    }
                                    //页面加载时调用
                                    LookCourse(1);
                                    //看视频的时候 用到
                                    setInterval(LookCourse, 60000);//60秒执行一次
                                }else{
                                    //试听未购买
                                    if(OverFlowAuth == 0){  //所有用户可试看
                                        var pId = Datas.PolyvVID;
                                        var sId = Datas.StuId;
                                        var OverTime = parseFloat(Datas.OverFlowTime) * 60;
                                        if (IsPC() == true) {
                                            player = polyvObject('#CuPlayer').videoPlayer({
                                                'width': '100%',
                                                'height': '100%',
                                                'vid': pId,
                                                'flashvars': { 'autoplay': 'true', 'watchEndTime': OverTime },
                                                'session_id': sId
                                            });
                                            window.s2j_onPlayOver = function () {
                                                $('#contentPlayOver').show();
                                            }
                                        } else {
                                            player = polyvObject('#CuPlayer').videoPlayer({
                                                'width': '100%',
                                                'height': '100%',
                                                'vid': pId,
                                                'df': 1,
                                                'flashvars': { 'autoplay': 'true', 'param2': sId, 'watchEndTime': OverTime },
                                                'params': { 'param2': sId },
                                                'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                            });
                                            window.s2j_onPlayOver = function () {
                                                $('#contentPlayOver').show();
                                            }
                                        }
                                    }else{  //注册用户可试看
                                        if(getCookie('StuId') && getCookie('StuId') != 'undefined'){
                                            var pId = Datas.PolyvVID;
                                            var sId = Datas.StuId;
                                            var OverTime = parseFloat(Datas.OverFlowTime) * 60;
                                            if (IsPC() == true) {
                                                player = polyvObject('#CuPlayer').videoPlayer({
                                                    'width': '100%',
                                                    'height': '100%',
                                                    'vid': pId,
                                                    'flashvars': { 'autoplay': 'true', 'watchEndTime': OverTime },
                                                    'session_id': sId
                                                });
                                                window.s2j_onPlayOver = function () {
                                                    $('#contentPlayOver').show();
                                                }
                                            } else {
                                                player = polyvObject('#CuPlayer').videoPlayer({
                                                    'width': '100%',
                                                    'height': '100%',
                                                    'vid': pId,
                                                    'df': 1,
                                                    'flashvars': { 'autoplay': 'true', 'param2': sId, 'watchEndTime': OverTime },
                                                    'params': { 'param2': sId },
                                                    'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                                });
                                                window.s2j_onPlayOver = function () {
                                                    $('#contentPlayOver').show();
                                                }
                                            }
                                        }else{
                                            layer.msg('请先登录后试听');
                                        }
                                    }

                                }
                            }else if(Datas.IsOpen === 1){
                                if(UserSeeAuth == 0){   //免费，所有用户可观看
                                    var pId = Datas.PolyvVID;
                                    var sId = Datas.StuId;
                                    if (IsPC() == true) {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'session_id': sId
                                        });
                                    } else {
                                        player = polyvObject('#CuPlayer').videoPlayer({
                                            'width': '100%',
                                            'height': '100%',
                                            'vid': pId,
                                            'df': 1,
                                            'flashvars': { 'autoplay': 'true', 'param2': sId },
                                            'params': { 'param2': sId },
                                            'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                        });
                                    }
                                    //页面加载时调用
                                    LookCourse(1);
                                    //看视频的时候 用到
                                    setInterval(LookCourse, 60000);//60秒执行一次
                                }else{
                                    //注册用户可观看，免费
                                    if (getCookie('StuId') && getCookie('StuId') != 'undefined') {
                                        var pId = Datas.PolyvVID;
                                        var sId = Datas.StuId;
                                        if (IsPC() == true) {
                                            player = polyvObject('#CuPlayer').videoPlayer({
                                                'width': '100%',
                                                'height': '100%',
                                                'vid': pId,
                                                'session_id': sId
                                            });
                                        } else {
                                            player = polyvObject('#CuPlayer').videoPlayer({
                                                'width': '100%',
                                                'height': '100%',
                                                'vid': pId,
                                                'df': 1,
                                                'flashvars': { 'autoplay': 'true', 'param2': sId },
                                                'params': { 'param2': sId },
                                                'flashParams': { 'wmode': 'window', 'allowScriptAccess': 'always', 'allowFullScreen': 'false' }
                                            });
                                        }
                                        //页面加载时调用
                                        LookCourse(1);
                                        //看视频的时候 用到
                                        setInterval(LookCourse, 60000);//60秒执行一次
                                    }else{
                                        layer.msg('请先登录后观看');
                                    }
                                }
                            }
                        } else {
                            layer.msg(data.Message);
                        }
                    });
                } else {
                    layer.msg('cId不能为0！')
                }
            };
            function addComment(Course_ID, Chapter_ID, html) {
                $.ajax({
                    url: '/service/CourseComment/AddCourseComment',
                    type: 'post',
                    data: {
                        Course_ID: Course_ID,
                        Chapter_ID: Chapter_ID,
                        Content: html
                    },
                    success: function (data) {
                        if (data.SuccessResponse) {
                            $('.CommentMsg h5').html(data.Message);
                            $('.hideCommentBtn').click();
                            $('.CommentClose').click();
                            $('.commentTxt').val(' ')
                        } else {
                            $('.CommentMsg h5').html(data.Message);
                            $('.hideCommentBtn').click();
                        }
                    }
                });
            }
            function addQuestion(PID, Course_ID, Chapter_ID, html) {
                $.ajax({
                    url: '/service/CourseInterlocution/AddCourseInterlocution',
                    type: 'post',
                    data: {
                        PID: PID,
                        Course_ID: Course_ID,
                        Chapter_ID: Chapter_ID,
                        Content: html
                    },
                    success: function (data) {
                        if (data.SuccessResponse) {
                            $('.CommentMsg h5').html(data.Message);
                            $('.hideCommentBtn').click();
                            $('.QuestionClose').click();
                            $('.questionTxt').val(' ')
                        } else {
                            $('.CommentMsg h5').html(data.Message);
                            $('.hideCommentBtn').click();
                        }
                    }
                });
            }

            toggleCatalog();
            $('.footerBox').on('click', function(){
                var id = $(this).find('a').data('id');
                var loginState = getCookie("StuId");
                    if (!loginState && loginState != 'undefined') {
                        layer.open({
                            type: 2,
                            area: ['400px', '550px'],
                            fixed: false, //不固定
                            maxmin: true,
                            title: '欢迎登陆',
                            content: '../login_model.html',
                        });
                    } else {
                        $.post("/service/Order/PutInShoppingCar", { Course_IDs: id }, function (res) {
                            if (res.SuccessResponse) {
                                layer.msg('添加成功')
                                location.href = '../Order/step11.shtml'
                            } else {
                                layer.msg(res.Message)
                            }
                        })
                    }
            });
            $('.commentSubmit').on('click', function () {
                var html = $('.commentTxt').val();
                var Course_ID = GetQueryString('Course_ID');
                var Chapter_ID = C_ID;
                addComment(Course_ID, Chapter_ID, html);
            });
            $('.questionSubmit').on('click', function () {
                var html = $('.questionTxt').val();
                var Course_ID = GetQueryString('Course_ID');
                var Chapter_ID = C_ID;
                var PID = P_ID;
                addQuestion(PID, Course_ID, Chapter_ID, html);
            });
            //$('.Question1').click(function () {
            //    console.log(player.j2s_realPlayVideoTime());
            //})

            //setTimeout(function () {
            //    //滚动条美化部分
            //    $(".Video-Catalog").mCustomScrollbar({
            //        mouseWheel: true,
            //    });
            //    $(".tab-content .active .Video-sro").mCustomScrollbar({
            //        mouseWheel: true,
            //    });

            //}, 0);
        });
        //直播有用的讨论
        //$(".Video-memberAndTalk-top>li:not(.active)").one("click", function () {
        //    setTimeout(function () {
        //        $(".tab-content .active .Video-sro").mCustomScrollbar({
        //            mouseWheel: true,
        //        });
        //    }, 0)
        //});
        //读取COOKIE方法
        function getCookie(name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg))
                return unescape(arr[2]);
            else
                return null;
        }
        var miaoInt = 0;
        var oldInt = 0;
        function LookCourse(id) {
            if (getCookie('StuId') && getCookie('StuId') != 'undefined') {
                var CourseChapters_ID = GetQueryString('ID');
                var LookTime;
                if (id === 1) {
                    LookTime = 0;
                } else {
                    LookTime = player.j2s_realPlayVideoTime();//播放时间
                }
                //var Duration = player.j2s_getDuration();//视频时长
                maioInt = LookTime - oldInt;
                oldInt = LookTime;
                $.post('/service/WebCourse/SaveCourse_Look', { CourseChapters_ID: CourseChapters_ID, LookType: 0, LookTime: maioInt, IP: '' }, function (data) {
                    if (!data.SuccessResponse) {
                        layer.msg(data.Message);
                    }
                })
            }
        }


        window.s2j_onVideoPlay = function(){
            $('#contentPlayOver').hide();
        }



    </script>
