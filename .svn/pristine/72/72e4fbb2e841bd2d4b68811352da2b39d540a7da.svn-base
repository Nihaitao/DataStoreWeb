<script>
    require_js_file(["table", "form", "jquery", "tableExt"], function (fnr) {
        //ele判断元素，clsClass名
        function hasClass(ele, cls) {
            return (new RegExp('(\\s|^)' + cls + '(\\s|$)')).test(ele.className);
        }
        function removeClass(ele, cls) {
            if (hasClass(ele, cls)) {
                var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');
                ele.className = ele.className.replace(reg, ' ');
            }
        }
        function addClass(ele, cls) {
            if (!hasClass(ele, cls)) {
                ele.className += ' ' + cls;
            }
        }
        ///参数获取
        function GetQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }

        function delAllCookie() {
            var myDate = new Date();
            myDate.setTime(-1000);//设置时间
            var data = document.cookie;
            var dataArray = data.split("; ");
            for (var i = 0; i < dataArray.length; i++) {
                var varName = dataArray[i].split("=");
                document.cookie = varName[0] + "=''; expires=" + myDate.toGMTString();
            }
        }

        var setTime;

        window.rootApp = new Vue({
            el: '#root',
            data: {
                loginState: false,
                headPhoto: '',
                msg: '',
                someOrderData: {
                    orderId: '',
                    addTime: ''
                },
                layoutData: {}
            },
            ready: function () {
                this.checkLogin();
                this.setSecondNum();
            },
            methods: {
                //退出
                exit: function () {
                    delAllCookie();
                    location.href = '../Login.html';
                },

                //查询stuID
                checkLogin: function () {
                    _this = this;
                    if (fnr.Cookie.get('StuId') && fnr.Cookie.get('StuId') != 'undefined') {
                        _this.loginState = true;
                        $.get("/service/Student/GetStudentInfo", function (res) {
                            if (res.SuccessResponse) {
                                _this.msg = 'hi，' + res.ReturnData.Data.Name;
                                _this.headPhoto = res.ReturnData.Data.HeadImg ? res.ReturnData.Data.HeadImg : '../Resources/images/photo.png'
                                _this.loginState = true;
                            }
                        });
                    }
                },

                //定时器
                setSecondNum: function () {
                    var n = 5;
                    var intTime = setInterval(function () {
                        n--;
                        $('.secondNum').html(n);
                        if (n <= 0) {
                            clearInterval(intTime);
                            //location.href = '../index.html';
                        }
                    }, 1000);
                }
            }
        });
        LayoutFn()
    });
</script>
