<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/layui.js"></script>
<link href="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/css/layui.css" rel="stylesheet" />
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/require_config.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/Controls/Common.js"></script>
<script>
    var dictionaryMapping = new Array();
    dictionaryMapping["Source_ID"] = "Source_ID";
    dictionaryMapping["PhaseType_ID"] = "PhaseType_ID";
    dictionaryMapping["Importance_ID"] = "Importance_ID";
    dictionaryMapping["ConnectionLevel_ID"] = "ConnectionLevel_ID";
    dictionaryMapping["Education_ID"] = "Education_ID";

    require_js_file(["table", "form", "jquery", "tableExt", "laydate", "customUtil", "upload"],
    function (fnr) {

        var laydate = layui.laydate;
        laydate.render({
            elem: "#EffDate"
        });
        var $ = layui.jquery;
        var upload = layui.upload;
        function trim(str) {
            return str.replace(/(^\s*)|(\s*$)/g, "");
        }
        function isPhoneNum(str) {
            return /^1[3|4|5|7|8][0-9]{9}$/.test(str)
        }
        window.rootApp = new Vue({
            el: "#root",
            data: {
                isDisplay: false,
                submitNewForm: {
                    Sex: '1',
                    NowProvince: '',
                    NowCity: "",
                    NowArea: "",
                    NowAddress: ''
                },
                submitForm: {
                    ID: ''
                },
                submitModify: {
                    ID: GetQueryString('ID'),
                    SystemGroup_ID: GetQueryString('SystemGroup_ID') ? GetQueryString('SystemGroup_ID') : 0
                },
                formData: {
                    common: [],
                    other: []
                },
                SelectSex: SelectSex(),
                provinceSetting: SelectProvinceBind(),//省份
                citySetting: SelectCityBind(),//城市
                areaSetting: SelectDistrictBind(),//区域
                NewInfoData: [],
                modifyArr: [],
                uploadElements: []
            },
            ready: function () {
                this.submitForm.ID = GetQueryString('ID');
                this.modifyClients();
            },
            methods: {
                provinceChange: function (o) {
                    var index = o.tag.replace('Province', '');
                    var cityTag = 'City' + index;
                    var areaTag = 'Area' + index;
                    var self = this;
                    if (self.submitNewForm.NowProvince == "") {
                        self.citySetting.remote.url = "";
                        self.citySetting.remote.data = {};
                    } else {
                        fnr.mergeJson(SelectCityBind(self.submitNewForm.NowProvince), self.citySetting);//合并ajax值
                    }
                    self.submitNewForm.NowCity = "";
                    self.submitNewForm.NowArea = "";
                    fnr.findComponent(self, cityTag).reset();
                },
                cityChange: function (o) {
                    var index = o.tag.replace('City', '');
                    var areaTag = 'Area' + index;
                    var self = this;
                    if (self.submitNewForm.NowCity == "") {
                        self.areaSetting.remote.url = "";
                        self.areaSetting.remote.data = {};
                    } else {
                        fnr.mergeJson(SelectDistrictBind(self.submitNewForm.NowCity), self.areaSetting);//合并ajax值
                    }
                    self.submitNewForm.NowArea = "";
                    fnr.findComponent(self, areaTag).reset();
                },

                createSetting: function (fieldName) {
                    return {
                        search: true,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Description' },
                        remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: dictionaryMapping[fieldName] } }
                    }
                },

                toggleDisplay: function () {
                    this.isDisplay = this.isDisplay ? false : true
                },
                //修改客户
                submit: function (e) {
                    var self = this;
                    if (!layui.form.checkInput(e)) {
                        return false;
                    }
                    var list = [];
                    $.each(self.formData.common, function (k, v) {
                        if (v.FieldName.toString().indexOf("customField_") == 0) {
                            list.push({ ItemName: v.FieldName, ItemValue: v.value });
                        }                       
                        self.submitModify[v.FieldName] = trim(v.value + '');
                    });

                    $.each(self.formData.other, function (k, v) {
                        if (v.FieldName.toString().indexOf("customField_") == 0) {
                            list.push({ ItemName: v.FieldName, ItemValue: v.value });
                        }
                        self.submitModify[v.FieldName] = trim(v.value + '');
                    });
                    self.submitModify.ListExtData = list;
                    if (self.submitNewForm['NowProvince']) {
                        self.submitModify['NowProvince'] = self.submitNewForm['NowProvince'];
                        self.submitModify['NowCity'] = self.submitNewForm['NowCity'];
                        self.submitModify['NowArea'] = self.submitNewForm['NowArea'];
                        self.submitModify['NowAddress'] = self.submitNewForm['NowAddress'];
                    }
                    if (self.submitModify.Phone) {
                        if (isPhoneNum(self.submitModify.Phone) == true) {
                            $.ajax({
                                url: '/service/crm/Client/UpdateClient',
                                type: 'post',
                                data: self.submitModify,
                                dataType: 'json',
                                success: function (data) {
                                    layer.msg(data.Message);
                                    if (data.SuccessResponse) {
                                        fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                        setTimeout(function () { fnr.closeDialog(); }, 500);
                                    } 
                                }
                            });
                        } else {
                            layer.msg('手机号码格式不正确');
                        }
                    } else {
                        $.ajax({
                            url: '/service/crm/Client/UpdateClient',
                            type: 'post',
                            data: self.submitModify,
                            dataType: 'json',
                            success: function (data) {
                                layer.msg(data.Message);
                                if (data.SuccessResponse) {
                                    fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                    fnr.closeDialog();
                                }
                            }
                        });
                    }
                },
                uploadRender: function () {
                    var self = this;
                    layui.$(".uploadBtn").each(function (k, v) {
                        var fieldName = $(v).data("fieldname");
                        var UploadInst = upload.render({
                            elem: v
                        , url: '/service/public/Upload/File/2'
                        , before: function (obj) {
                            //预读本地文件示例，不支持ie8
                            obj.preview(function (index, file, result) {
                                $(v).prev().attr('src', result); //图片链接（base64
                            });
                        }
                        , done: function (res) {
                            //如果上传失败
                            if (res.code > 0) {
                                return layer.msg('上传失败');
                            }
                            self.uploadElements[fieldName].value = res.Message;
                            //上传成功
                            $(v).prev().prev().val(res.Message);
                            //rootApp.HeaderSubmitForm.Favicon = res.Message
                            layer.msg('上传成功')
                        }
                        , error: function () {
                            //演示失败状态，并实现重传
                            var demoText = $('#upLoadText' + id);
                            $(v).next().html('<span style="color: #FF5722;">上传失败</span> <a class=" favicon-reload" style="cursor:pointer;">重试</a>');
                            $(v).next().find('.favicon-reload').on('click', function () {
                                UploadInst.upload();
                            });
                        }
                        });
                    });
                },
                //获取客户详细信息
                modifyClients: function () {
                    var self = this;
                    $.ajax({
                        url: '/service/crm/Client/GetCustomerInfoSet',
                        type: 'GET',
                        dataType: 'json',
                        data: { ID: GetQueryString('ID') },
                        success: function (data) {                           
                            if (data.SuccessResponse) {
                                var Datas = data.Data;
                                var childArr = [];
                                $.each(Datas, function (k, v) {
                                    if (v.Childs.length > 0) {
                                        $.each(v.Childs, function (_k, _v) {
                                            if (_v.FieldType == 107) {
                                                self.uploadElements[_v.FieldName] = _v;
                                            }
                                            if (_v.DataSource != null) {
                                                _v.DataSource = eval(_v.DataSource);
                                            }
                                            childArr.push(_v);
                                            if (_v.FieldName !== null) {
                                                if (_v.FieldName.split('_').length > 1 && _v.FieldName.toString().indexOf('customField_') != 0) {
                                                    if (_v.Value!=null){
                                                        self.submitNewForm[_v.FieldName] = parseFloat(_v.Value.split(',')[1]);
                                                    }
                                                } else if (_v.FieldName !== 'Sex') {
                                                    self.submitNewForm[_v.FieldName] = _v.Value == 0 ? '' : _v.Value;
                                                } else {
                                                    self.submitNewForm[_v.FieldName] = _v.Value;
                                                }
                                            }
                                        })
                                    }
                                })
                                $.each(childArr, function (k, v) {
                                    if (v.FieldType > 0) {

                                        v["value"] = self.submitNewForm[v.FieldName];
                                        if (isNaN(v["value"])) {
                                            
                                            v["Value"] = '';
                                        }
                                        if (v.IsEnable == 1) {
                                            (v.Common == 1 ? self.formData.common : self.formData.other)
                                                .push(v);
                                        }



                                        
                                    }

                                    if (v.FieldType == 11) {
                                        if (v.value != null && v.value != "") {
                                            var splitV = v.value.split('|');
                                            self.submitNewForm['NowProvince'] = splitV[0].split(',')[0];
                                            
                                            fnr.mergeJson(SelectCityBind(self.submitNewForm.NowProvince), self.citySetting);//合并ajax值
                                            self.submitNewForm['NowCity'] = splitV[1].split(',')[0];
                                            fnr.mergeJson(SelectDistrictBind(self.submitNewForm.NowCity), self.areaSetting);//合并ajax值
                                            self.submitNewForm['NowArea'] = splitV[2].split(',')[0];
                                            self.submitNewForm['NowAddress'] = splitV[3];
                                        }
                                    }
                                    
                                });
                                self.$nextTick(function () {
                                    self.uploadRender();
                                });
                            }
                        },
                        error: function () {
                            alert("error");
                        }
                    });
                }
            }
        });
    });
</script>