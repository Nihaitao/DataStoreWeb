            }
            function isPhoneNum(str) {
                return /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/.test(str)
            }
            Vue.filter('format',
                function(time, format) {
                    if (time == undefined || time == null || !time) {
                        return '';
                    }
                    var finalTime;
                    if (typeof (time) == 'string') {
                        time = time.replace("T", " ");
                        time = time.replace("Z", " ");
                        var timeIOS = time.replace(/\-/g, "/");
                        finalTime = new Date(timeIOS);
                    } else {
                        finalTime = time;
                    }

                    var t = new Date(finalTime);
                    var tf = function(i) { return (i < 10 ? '0' : '') + i };
                    return format.replace(/yyyy|MM|dd|HH|mm|ss/g,
                        function(a) {
                            switch (a) {
                            case 'yyyy':
                                return tf(t.getFullYear());
                                break;
                            case 'MM':
                                return tf(t.getMonth() + 1);
                                break;
                            case 'mm':
                                return tf(t.getMinutes());
                                break;
                            case 'dd':
                                return tf(t.getDate());
                                break;
                            case 'HH':
                                return tf(t.getHours());
                                break;
                            case 'ss':
                                return tf(t.getSeconds());
                                break;
                            }
                        });
                })

            var rootApp = new Vue({
                el: "#root",
                data: {
                    isDisplay: false,
                    formData: {
                        common: [],
                        other: []
                    },
                    provinceSetting: SelectProvinceBind(),//省份
                    citySetting: SelectCityBind(),//城市
                    areaSetting: SelectDistrictBind(),//区域
                    ClueStatusSetting: {
                        search: true,
                        selectedIndex: 0,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Description' },
                        remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: 'ClueStatus_ID' } }
                    },

                    SourceSetting: {
                        search: true,
                        selectedIndex: 0,
                        options: [],
                        fields: { root: 'Data', key: 'ID', value: 'Description' },
                        remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: 'Source_ID' } }
                    },
                    submitNewForm: {
                        ClueStatus_ID: 0,
                        Source_ID: 0,
                        NowProvince: 0,
                        NowCity: 0,
                        NowArea: 0,
                        NowAddress: ''
                    }
                },


                ready: function() {
                    this.loadCustomForm();
                },
                methods: {
                    provinceChange: function (o) {
                        var index = o.tag.replace('Province', '');
                        var cityTag = 'City' + index;
                        var areaTag = 'Area' + index;
                        var self = this;
                        if (self.submitNewForm.NowProvince == "") {
                            self.citySetting.remote.url = "";
                            self.citySetting.remote.data = {};
                            fnr.findComponent(self, cityTag).reset();
                        } else {
                            fnr.mergeJson(SelectCityBind(self.submitNewForm.NowProvince), self.citySetting);//合并ajax值
                            fnr.findComponent(self, cityTag).reset();
                        }
                        //if (self.areaSetting.remote != undefined) {
                        //    self.areaSetting.remote.url = "";
                        //    self.areaSetting.remote.data = {};
                        //    fnr.findComponent(self, cityTag).reset();
                        //}
                    },
                    cityChange: function (o) {
                        var index = o.tag.replace('City', '');
                        var areaTag = 'Area' + index;
                        var self = this;
                        if (self.submitNewForm.NowCity == "") {
                            self.areaSetting.remote.url = "";
                            self.areaSetting.remote.data = {};
                        } else {
                            fnr.mergeJson(SelectDistrictBind(self.submitNewForm.NowCity), self.areaSetting);//合并ajax值
                        }
                        fnr.findComponent(self, areaTag).reset();
                    },

                    getSubmitData: function() {
                        var self = this;
                        var submitData = {};
                        var list = [];
                        $.each(self.formData.common,
                            function(k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined)
                                        {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;

                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });

                        $.each(self.formData.other,
                            function(k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        //submitData.ListExtData[v.FieldName] = v.value;
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined)
                                        {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });
                        submitData.ListExtData = list;
                        return submitData;
                    },
                    toggleDisplay: function() {
                        this.isDisplay = !this.isDisplay;
                    },
                    loadCustomForm: function() {
                        var self = this;
                        $.ajax({
                            url: '/service/public/CustomSettings/GetLayoutConfig',
                            type: 'get',
                            data: { BusType: 2, Status: 1 },
                            dataType: 'json',
                            success: function (data) {
                                if (data.SuccessResponse) {
                                    var childArr = [];                                   
                                    var newDatas = data.Data;
                                    $.each(newDatas,
                                        function (key, value) {
                                            $.each(value.Childs,
                                                function (childKey, childValue) {
                                                    if (childValue.DataSource != null) {
                                                        childValue.DataSource = eval(childValue.DataSource);
                                                        childArr.push(childValue);
                                                    } else {
                                                        childArr.push(childValue);
                                                    }
                                                });
                                        });
                                    $.each(childArr,
                                        function (key, value) {
                                            if (value.FieldType > 0) {
                                                if (value.IsEnable == 1) {
                                                    (value.Common == 1 ? self.formData.common : self.formData.other)
                                                        .push(value);
                                                       
                                                }
                                            }
                                        });
                                } 
                            }
                        });
                    },
                    submit: function (e) {
                        var self = this;
                        if (!layui.form.checkInput(e)) {
                            return false;
                        }
                        var AddDatas = $.extend(self.getSubmitData(), self.submitNewForm);
                        if (AddDatas.Phone)
                        {
                            if(isPhoneNum(AddDatas.Phone) == true)
                            {
                                $.ajax({
                                    url: '/service/crm/Clue/AddClue',
                                    type: 'post',
                                    data: AddDatas,
                                    dataType: 'json',
                                    success: function (data) {
                                        if (data.SuccessResponse) {
                                            fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                            fnr.closeDialog();
                                        }
                                        else {
                                            layer.msg(data.Message);
                                        }
                                    }
                                });
                            } else {
                                layer.msg("手机号码格式不正确！");
                            }
                        } else {
                            $.ajax({
                                url: '/service/crm/Clue/AddClue',
                                type: 'post',
                                data: AddDatas,
                                dataType: 'json',
                                success: function (data) {
                                    if (data.SuccessResponse) {
                                        fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                        fnr.closeDialog();
                                    }
                                    else {
                                        layer.msg(data.Message);
                                    }
                                }
                            });
                        } 
                    }

                }
            });
        });
</script>