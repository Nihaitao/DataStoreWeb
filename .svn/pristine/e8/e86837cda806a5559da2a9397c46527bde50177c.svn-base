<script>
    var dictionaryMapping = new Array();
    dictionaryMapping["Source_ID"] = "Source_ID";
    dictionaryMapping["PhaseType_ID"] = "PhaseType_ID";
    dictionaryMapping["Importance_ID"] = "Importance_ID";
    dictionaryMapping["ConnectionLevel_ID"] = "ConnectionLevel_ID";
    dictionaryMapping["Education_ID"] = "Education_ID";

    require_js_file(["table", "form", "jquery", "tableExt", "laydate", "customUtil"],
        function (fnr) {

            var laydate = layui.laydate;
            laydate.render({
                elem: "#EffDate"
            });
            var $ = layui.jquery;
            var form = layui.form;
            function trim(str) {
                return str.replace(/(^\s*)|(\s*$)/g, "");
            }
            Vue.filter('format',
                function (time, format) {
                    if (time == undefined || time == null || !time) {
                        return '';
                    }
                    var finalTime;
                    if (typeof (time) == 'string') {
                        time = time.replace("T", " ");
                        time = time.replace("Z", " ");
                        var timeIOS = time.replace(/\-/g, "/");
                        finalTime = new Date(timeIOS);
                    } else {
                        finalTime = time;
                    }

                    var t = new Date(finalTime);
                    var tf = function (i) { return (i < 10 ? '0' : '') + i };
                    return format.replace(/yyyy|MM|dd|HH|mm|ss/g,
                        function (a) {
                            switch (a) {
                                case 'yyyy':
                                    return tf(t.getFullYear());
                                    break;
                                case 'MM':
                                    return tf(t.getMonth() + 1);
                                    break;
                                case 'mm':
                                    return tf(t.getMinutes());
                                    break;
                                case 'dd':
                                    return tf(t.getDate());
                                    break;
                                case 'HH':
                                    return tf(t.getHours());
                                    break;
                                case 'ss':
                                    return tf(t.getSeconds());
                                    break;
                            }
                        });
                })

            var rootApp = new Vue({
                el: "#root",
                data: {
                    isDisplay: false,
                    formData: {
                        common: [],
                        other: []
                    },
                    SelectSex: SelectSex(),
                    provinceSetting: SelectProvinceBind(),//省份
                    citySetting: SelectCityBind(),//城市
                    areaSetting: SelectDistrictBind(),//区域
                    submitNewForm: {
                        Sex: 1,
                        NowProvince: 0,
                        NowCity: 0,
                        NowArea: 0,
                        NowAddress: ''
                    },
                    ClueID: 0,
                },

                ready: function () {
                    this.loadCustomForm();
                    this.ClueID = GetQueryString('ID');
                },
                methods: {
                    createSetting: function (fieldName) {
                        return {
                            search: true,
                            selectedIndex: 0,
                            options: [],
                            fields: { root: 'ReturnData.Data', key: 'ID', value: 'Description' },
                            remote: { url: '/service/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: dictionaryMapping[fieldName] } }
                        }
                    },

                    provinceChange: function (o) {
                        var index = o.tag.replace('Province', '');
                        var cityTag = 'City' + index;
                        var areaTag = 'Area' + index;
                        var self = this;
                        if (self.submitNewForm.NowProvince == "") {
                            self.citySetting.remote.url = "";
                            self.citySetting.remote.data = {};
                            fnr.findComponent(self, cityTag).reset();
                        } else {
                            fnr.mergeJson(SelectCityBind(self.submitNewForm.NowProvince), self.citySetting);//合并ajax值
                            fnr.findComponent(self, cityTag).reset();
                        }
                        if (self.areaSetting.remote != undefined) {
                            self.areaSetting.remote.url = "";
                            self.areaSetting.remote.data = {};
                            fnr.findComponent(self, cityTag).reset();
                        }
                    },
                    cityChange: function (o) {
                        var index = o.tag.replace('City', '');
                        var areaTag = 'Area' + index;
                        var self = this;
                        if (self.submitNewForm.NowCity == "") {
                            self.areaSetting.remote.url = "";
                            self.areaSetting.remote.data = {};
                        } else {
                            fnr.mergeJson(SelectDistrictBind(self.submitNewForm.NowCity), self.areaSetting);//合并ajax值
                        }
                        fnr.findComponent(self, areaTag).reset();
                    },
                    turnClient: function () {
                        var self = this;
                        $.ajax({
                            url: '/service/Clue/GetClueDataByID',
                            type: 'GET',
                            dataType: 'json',
                            data: { ID: GetQueryString('ID') },
                            success: function (resp) {
                                resp = resp.ReturnData.Data;
                                $.each(self.formData.common, function (k, v) {
                                    if (v.FieldName == "PhaseType_ID") {
                                        v.value = parseInt(resp["ClueStatus_ID"]);
                                        self.submitNewForm.PhaseType_ID = v.value;
                                    }
                                    else if (v.FieldName == "Source_ID") {
                                        v.value = parseInt(resp["Source_ID"]);
                                        self.submitNewForm.Source_ID = v.value;
                                    }
                                    else if (resp[v.FieldName] != undefined) {
                                        v.value = resp[v.FieldName];
                                    }
                                });

                                $.each(self.formData.other, function (k, v) {
                                    if (v.FieldName == "PhaseType_ID") {  
                                        v.value = parseInt(resp["ClueStatus_ID"]);
                                        self.submitNewForm.PhaseType_ID = v.value;
                                    }
                                    else if (v.FieldName == "Source_ID") {
                                        v.value = parseInt(resp["Source_ID"]);
                                        self.submitNewForm.Source_ID = v.value;
                                    }
                                    else if (resp[v.FieldName] != undefined) {
                                        v.value = resp[v.FieldName];
                                    }
                                });
                            }
                        });
                    },
                    getSubmitData: function () {
                        var self = this;
                        var submitData = {};
                        var list = [];
                        $.each(self.formData.common,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });

                        $.each(self.formData.other,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });
                        submitData.ListExtData = JSON.stringify(list);
                        return submitData;
                    },
                    toggleDisplay: function () {
                        this.isDisplay = !this.isDisplay;
                    },
                    loadCustomForm: function () {
                        var self = this;
                        $.ajax({
                            url: '/service/CustomSettings/GetLayoutConfig',
                            type: 'get',
                            data: { BusType: 1, Other: 1 },
                            dataType: 'json',
                            success: function (data) {
                                if (data.ReturnData.Data) {                                   
                                    var childArr = [];
                                    if (data.SuccessResponse) {
                                        var newDatas = data.ReturnData.Data;
                                        $.each(newDatas,
                                            function (key, value) {
                                                $.each(value.Childs,
                                                    function (childKey, childValue) {
                                                        if (childValue.DataSource != null) {
                                                            childValue.DataSource = eval(childValue.DataSource);
                                                            childArr.push(childValue);
                                                        } else {
                                                            childArr.push(childValue);
                                                        }
                                                    });
                                            });
                                        $.each(childArr,
                                            function (key, value) {
                                                if (value.FieldType > 0) {
                                                    if (value.IsEnable == 1) {
                                                        value.value = '';
                                                        (value.Common == 1 ? self.formData.common : self.formData.other)
                                                            .push(value);
                                                    }
                                                }
                                            });
                                        ///获取线索信息
                                        if (self.ClueID > 0) {
                                            self.turnClient();
                                        }
                                    }
                                } else {
                                    layer.msg('data为空');
                                }
                            }
                        });
                    },
                    submit: function (e) {
                        var self = this;
                        if (!layui.form.checkInput(e)) {
                            return false;
                        }
                        var AddDatas = $.extend(self.getSubmitData(), self.submitNewForm)
                        $.ajax({
                            url: '/service/MyClient/AddClient',
                            type: 'post',
                            data: AddDatas,
                            dataType: 'json',
                            success: function (data) {
                                console.log(data);
                                if (data.SuccessResponse) {
                                    fnr.callDialog('refresh', { status: data.SuccessResponse, ClientID: data.ReturnData.Data["ID"], Message: data.Message });
                                    if (data.SuccessResponse) {
                                        fnr.closeDialog();
                                    }
                                } else {
                                    layer.msg("添加失败："+data.Message);
                                }
                            }
                        });
                    }
                }
            });
        });
</script>