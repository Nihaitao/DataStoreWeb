<script>
    var dictionaryMapping = new Array();
    dictionaryMapping["OpportunityPhase_ID"] = "OpportunityPhase_ID";
    dictionaryMapping["OpportunitySource_ID"] = "OpportunitySource_ID";
    dictionaryMapping["OpportunityType_ID"] = "OpportunityType_ID";

    require_js_file(["table", "form", "jquery", "tableExt", "laydate", "customUtil", "upload"],
        function (fnr) {

            var laydate = layui.laydate;
            laydate.render({
                elem: "#EffDate"
            });
            var $ = layui.jquery;
            var form = layui.form;
            var upload = layui.upload;
            function trim(str) {
                return str.replace(/(^\s*)|(\s*$)/g, "");
            }
            function isPhoneNum(str) {
                return /^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/.test(str)
            }
            Vue.filter('format',
                function (time, format) {
                    if (time == undefined || time == null || !time) {
                        return '';
                    }
                    var finalTime;
                    if (typeof (time) == 'string') {
                        time = time.replace("T", " ");
                        time = time.replace("Z", " ");
                        var timeIOS = time.replace(/\-/g, "/");
                        finalTime = new Date(timeIOS);
                    } else {
                        finalTime = time;
                    }

                    var t = new Date(finalTime);
                    var tf = function (i) { return (i < 10 ? '0' : '') + i };
                    return format.replace(/yyyy|MM|dd|HH|mm|ss/g,
                        function (a) {
                            switch (a) {
                                case 'yyyy':
                                    return tf(t.getFullYear());
                                    break;
                                case 'MM':
                                    return tf(t.getMonth() + 1);
                                    break;
                                case 'mm':
                                    return tf(t.getMinutes());
                                    break;
                                case 'dd':
                                    return tf(t.getDate());
                                    break;
                                case 'HH':
                                    return tf(t.getHours());
                                    break;
                                case 'ss':
                                    return tf(t.getSeconds());
                                    break;
                            }
                        });
                })

            window.rootApp = new Vue({
                el: "#root",
                data: {
                    upLoadSrc: '123',
                    isDisplay: false,
                    formData: {
                        common: [],
                        other: []
                    },
                    SelectSex: SelectSex(),
                   
                    submitNewForm: {

                    },
                    ClueID: 0,
                    uploadElements:[]
                },

                ready: function () {
                    this.loadCustomForm();
                    //this.ClueID = GetQueryString('ID');
                    //if (GetQueryString('SystemGroup_ID')) {
                    //    this.submitNewForm.SystemGroup_ID = GetQueryString('SystemGroup_ID');
                    //}
                },
                methods: {
                    createSetting: function (fieldName) {
                        return {
                            search: true,
                            selectedIndex: 0,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Description' },
                            remote: { url: '/service/public/HDictionary/GetHDictionaryList', data: { Valid: 1, ColumnName: dictionaryMapping[fieldName] } }
                        }
                    },
                    AccountSetting:function(){
                        return {
                            search: true,
                            selectedIndex: 0,
                            options: [],
                            fields: { root: 'Data', key: 'ID', value: 'Name' },
                            remote: { url: '/service/crm/Opportunity/GetMyClientList'}
                        }
                    },
                    getSubmitData: function () {
                        var self = this;
                        var submitData = {};
                        var list = [];
                        $.each(self.formData.common,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });

                        $.each(self.formData.other,
                            function (k, v) {
                                if (v.FieldName != null) {
                                    if (v.FieldName.toString().indexOf("customField_") == 0 && v.value != undefined) {
                                        list.push({ ItemName: v.FieldName, ItemValue: v.value + '' })
                                    } else {
                                        if (v.value != undefined) {
                                            v.value = trim(v.value + '');
                                            submitData[v.FieldName] = v.value;
                                        } else {
                                            submitData[v.FieldName] = v.value;
                                        }
                                    }
                                }
                            });
                        submitData.ListExtData = list;
                        return submitData;
                    },
                    toggleDisplay: function () {
                        this.isDisplay = !this.isDisplay;
                    },
                    loadCustomForm: function () {
                        var self = this;
                        $.ajax({
                            url: '/service/public/CustomSettings/GetLayoutConfig',
                            type: 'get',
                            data: { BusType: 5, Status: 1 },
                            dataType: 'json',
                            success: function (data) {
                                if (data.SuccessResponse) {
                                    var childArr = [];
                                    var newDatas = data.Data;
                                    $.each(newDatas,
                                        function (key, value) {
                                            $.each(value.Childs,
                                                function (childKey, childValue) {
                                                    if (childValue.FieldType == 107) {
                                                        self.uploadElements[childValue.FieldName] = childValue;
                                                    }
                                                    if (childValue.DataSource != null) {
                                                        childValue.DataSource = eval(childValue.DataSource);
                                                        childArr.push(childValue);
                                                    } else {
                                                        childArr.push(childValue);
                                                    }
                                                });
                                        });
                                    $.each(childArr,
                                        function (key, value) {
                                            if (value.FieldType > 0) {
                                                if (value.IsEnable == 1) {
                                                    value.value = '';
                                                    (value.Common == 1 ? self.formData.common : self.formData.other)
                                                        .push(value);
                                                }
                                            }
                                        });
                                  

                                    //self.$nextTick(function () {
                                    //    self.uploadRender();
                                    //});
                                }
                            }
                        });
                    },
                    submit: function (e) {
                        var self = this;
                        if (!layui.form.checkInput(e)) {
                            return false;
                        }
                        var AddDatas = $.extend(self.getSubmitData(), self.submitNewForm);
                        $.ajax({
                            url: '/service/crm/Opportunity/AddOpportunity',
                            type: 'post',
                            data: AddDatas,
                            dataType: 'json',
                            success: function (data) {
                                if (data.SuccessResponse) {
                                    fnr.callDialog('refresh', { status: data.SuccessResponse, msg: data.Message });
                                    setTimeout(function () { fnr.closeDialog(); }, 500);
                                    //fnr.callDialog('refresh',data);
                                    //fnr.closeDialog();
                                } else {
                                    alert(2)
                                    layer.msg("添加失败：" + data.Message);
                                }
                            }
                        });
                    }
                }
            });
        });
</script>