main {
  width: 100%;
  height: 100%;
  padding-bottom: 0.844827rem;
}
main .tip {
  position: relative;
  z-index: 999 !important;
}
main .tip ul {
  width: 100%;
  border-bottom: 0.017241rem solid #c3c3c3;
}
main .tip ul li {
  position: relative;
  width: 33%;
  float: left;
  font-size: 0.224137rem;
  text-align: center;
  line-height: 0.689655rem;
}
main .tip ul li:nth-child(2) {
  border-left: 0.017241rem solid #dddddd;
  border-right: 0.017241rem solid #dddddd;
}
main .tip ul li.click {
  color: #f33f48;
}
main .tip ul li .down {
  position: absolute;
  left: 50%;
  bottom: -0.34rem;
  font-size: 0.172413rem;
  margin-left: -0.051724rem;
}
main .tip ul li .down img {
  width: 0.172413rem;
  height: 0.103448rem;
}
main .tip .side {
  background: #efeff4;
  position: absolute;
  top: 0.741379rem;
}
main .tip .side i {
  font-style: normal;
}
main .tip .side .side_p {
  width: 1.534482rem;
  float: left;
}
main .tip .side .side_p div {
  width: 1.534482rem;
  line-height: 1.275862rem;
  text-align: center;
  border-bottom: 0.017241rem solid #dddddd;
}
main .tip .side .side_p .active {
  background: #fff;
  border-left: 0.051724rem solid #f10013;
}
main .tip .side .side_show {
  margin-left: 1.534482rem;
  background: #fff;
  padding-bottom: 0.448275rem;
}
main .tip .side .side_show .side_p_show {
  float: left;
  margin: 0px 0.172413rem;
}
main .tip .side .side_show .side_p_show .diqu {
  padding-bottom: 0.172413rem;
}
main .tip .side .side_show .side_p_show .diqu i {
  float: left;
  width: 1.37931rem;
  line-height: 0.862068rem;
  background: #efeff4;
  margin-top: 0.172413rem;
  text-align: center;
  border-radius: 0.103448rem;
}
main .tip .side .side_show .side_p_show .diqu i:nth-child(3n-1) {
  margin: 0.172413rem 0.172413rem 0px;
}
main .tip .side .side_show .side_p_show .diqu div {
  line-height: 0.586206rem;
  padding-top: 0.344827rem;
}
main .explain {
  line-height: 0.5rem;
  font-size: 0.189655rem;
  text-align: center;
  background: #efeff4;
}
main .explain .choice {
  color: #f10013;
}
main .firstCon img {
  display: block;
  width: 2.413793rem;
  height: 1.37931rem;
  border-radius: 0.086206rem;
}
main .firstCon .international {
  padding: 0.25862rem 0px ;
  padding-left: 0.172413rem;
  border-bottom: 0.017241rem solid #efeff4;
}
main .firstCon .international .inter_English_describe {
  padding-left: 0.172413rem;
  width: 2.741379rem;
}
main .firstCon .international .inter_English_describe .inter_English_describe_person {
  position: relative;
}
main .firstCon .international .inter_English_describe h3 {
  font-size: 0.25862rem;
  line-height: 0.465517rem;
}
main .firstCon .international .inter_English_describe i {
  font-style: normal;
  font-size: 0.206896rem;
  color: #999999;
  margin-left: 0.275862rem;
}
main .firstCon .international .inter_English_describe i span {
  color: #f10013;
}
main .firstCon .international .inter_English_describe i span a {
  font-size: 0.206896rem;
  color: #f10013;
}
main .firstCon .international .inter_English_describe .price {
  color: #f10013;
  font-size: 0.172413rem;
  margin-left: 0.25862rem;
}
main .more_btn {
  position: relative;
}
main .more {
  position: absolute;
  width: 1.724137rem;
  line-height: 0.362068rem;
  top: 50%;
  left: 50%;
  margin-left: -0.862068rem;
  margin-top: 0.172413rem;
  text-align: center;
  border: none;
  font-size: 0.189655rem;
  border-radius: 0.068965rem;
}
main .mask {
  position: absolute;
  top: 2.413793rem;
  width: 100%;
  height: 8.844827rem;
  background: rgba(0, 0, 0, 0.5);
  z-index: 15;
}
.circle {
  width: 0.206896rem;
  height: 0.206896rem;
  position: absolute;
  border-radius: 50%;
  /*background: #999;*/
  top: 0.068965rem;
  border: 0.017241rem solid #ddd;
}
.left,
.right {
  width: 0.206896rem;
  height: 0.206896rem;
  position: absolute;
  top: 0px;
  left: 0px;
}
.pie_left {
  width: 0.206896rem;
  height: 0.206896rem;
  position: absolute;
  border-radius: 50%;
  top: 0px;
  left: 0px;
  background: #fff;
}
.pie_right {
  width: 0.206896rem;
  height: 0.206896rem;
  position: absolute;
  border-radius: 50%;
  top: 0px;
  left: 0px;
  background: #fff;
  z-index: 10;
}
.pie_right,
.right {
  clip: rect(0, auto, auto, 0.103448rem);
}
.pie_left,
.left {
  clip: rect(auto, 0.103448rem, auto, auto);
}
/*
*当top和left取值为auto时，相当于0
*当bottom和right取值为auto时，相当于100%
*/

 .tip .notList.active {
    color: #f01414;
    border-bottom: 3px solid #f01414;
}
 .tip .notList .down {
    display: none;
}
.tip .notList.active .down {
    display: block;
}
.percent {
    display: inline-block;
    margin-top: 0.038965rem;
}