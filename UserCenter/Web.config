<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="proxyServices" type="CT.HttpProxy.Configuration.ProxyConfiguration,CT.HttpProxy" />
  </configSections>
  <proxyServices proxyPath="service">
    <services>

      <!--<add prefix="edu" url="http://localhost:63795/" debug="true" />-->
      <!--<add prefix="hr" url="http://webhrcs.chutoukj.com/" debug="true" />-->
      <add prefix="datastore" url="http://localhost:63795/" debug="true" />
      <add prefix="*" url="http://47.97.163.229:8080/" />
    </services>
  </proxyServices>
  <system.web>
    <customErrors mode="Off"/>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <authentication mode="Forms">
      <forms loginUrl="Default.cshtml" defaultUrl="About.cshtml" />
    </authentication>
  </system.web>
  <system.webServer>
    <handlers>
      <add path="/service/*" type="CT.HttpProxy.ProxyHandler,CT.HttpProxy" verb="*" name="proxyHandler" />
    </handlers>
  </system.webServer>

</configuration>